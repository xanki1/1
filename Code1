                            v_12.Enabled = arg2
                        end
                    end
                end
            end
            return
        end
        for _, v_13 in pairs(Effects_2:GetChildren()) do
            if string.find(v_13.Name, "Enchant2") then
                v_13.Enabled = arg2
            end
        end
    end
end
function tbl_7.ChargeEffect(arg1, arg2) -- Line 822
    local Effects = arg1:FindFirstChild("Effects")
    if not Effects or not Effects:FindFirstChild("ChargeEffect") then
    else
        Effects.ChargeEffect.Enabled = arg2
    end
end
var161_upvw = tbl_7
function module_2_upvr.HandModelEffect(arg1, arg2, arg3, arg4, arg5) -- Line 831
    --[[ Upvalues[1]:
        [1]: var161_upvw (read and write)
    ]]
    if var161_upvw[arg3] and arg2:FindFirstChild("HandModel") then
        local var182
        if arg2.HandModel.Value then
            var182 = arg2.HandModel
            local Value_2 = var182.Value
            local function INLINED_2() -- Internal function, doesn't exist in bytecode
                var182 = Value_2:FindFirstChild("LeftGrip")
                return var182
            end
            if not arg5 or not INLINED_2() then
                var182 = Value_2:FindFirstChild("RightGrip")
            end
            if not var182 then return end
            var161_upvw[arg3](var182, arg4)
            if arg5 == nil and Value_2:FindFirstChild("LeftGrip") then
                var161_upvw[arg3](Value_2.LeftGrip, arg4)
                return
            end
        end
    end
end
